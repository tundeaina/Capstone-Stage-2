<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    tools:context="com.aina.adnd.popestimator.AOIReportFragment">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/subtitle"
        android:textColor="@color/primary_text"
        android:textSize="16sp"
        android:textStyle="bold"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="5dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/subsubtitle"
        android:layout_below="@+id/subtitle"
        android:textColor="@color/primary_text"
        android:textSize="16sp"
        android:layout_centerHorizontal="true" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/subsubtitle"
        android:orientation="horizontal"
        android:layout_marginTop="10dp">

        <WebView
            android:id="@+id/webView1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:layout_marginTop="20dp"/>

        <GridLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:columnCount="3"
            android:orientation="vertical"
            android:padding="@dimen/widget_margin"
            android:layout_weight="2"
            android:layout_marginTop="30dp"
            android:layout_marginStart="@dimen/grid_margin_right">


            <TextView
                android:layout_column="0"
                android:layout_row="0"
                android:layout_marginBottom="@dimen/grid_margin_bottom"
                android:layout_marginEnd="@dimen/grid_margin_right"
                android:textColor="@color/primary_text"
                />
            <TextView
                android:text="@string/counts"
                android:layout_column="1"
                android:layout_row="0"
                android:layout_marginBottom="@dimen/grid_margin_bottom"
                android:layout_marginEnd="@dimen/grid_margin_right"
                android:gravity="center_vertical|end"
                android:layout_gravity="end"
                android:textColor="@color/primary_text"/>
            <TextView
                android:text="@string/percentage"
                android:layout_column="2"
                android:layout_row="0"
                android:layout_marginBottom="@dimen/grid_margin_bottom"
                android:layout_marginEnd="@dimen/grid_margin_right"
                android:gravity="center_vertical|end"
                android:layout_gravity="end"
                android:textColor="@color/primary_text"/>


            <TextView
                android:text="@string/amerindian"
                android:layout_column="0"
                android:layout_row="1"
                android:layout_marginBottom="@dimen/grid_margin_bottom"
                android:layout_marginEnd="@dimen/grid_margin_right"
                android:textColor="@color/primary_text"
                />
            <TextView
                android:text="@string/amerindian"
                android:id="@+id/amerindian1"
                android:layout_column="1"
                android:layout_row="1"
                android:layout_marginBottom="@dimen/grid_margin_bottom"
                android:layout_marginEnd="@dimen/grid_margin_right"
                android:gravity="center_vertical|end"
                android:layout_gravity="end"
                android:textColor="@color/primary_text"/>
            <TextView
                android:text="@string/amerindian"
                android:id="@+id/amerindian2"
                android:layout_column="2"
                android:layout_row="1"
                android:layout_marginBottom="@dimen/grid_margin_bottom"
                android:layout_marginEnd="@dimen/grid_margin_right"
                android:gravity="center_vertical|end"
                android:layout_gravity="end"
                android:textColor="@color/primary_text"/>

            <TextView
                android:text="@string/asian"
                android:layout_column="0"
                android:layout_row="2"
                android:layout_marginBottom="@dimen/grid_margin_bottom"
                android:layout_marginEnd="@dimen/grid_margin_right"
                android:textColor="@color/primary_text"
                />
            <TextView
                android:id="@+id/asian1"
                android:layout_column="1"
                android:layout_row="2"
                android:layout_marginBottom="@dimen/grid_margin_bottom"
                android:layout_marginEnd="@dimen/grid_margin_right"
                android:layout_gravity="end"
                android:textColor="@color/primary_text"/>
            <TextView
                android:id="@+id/asian2"
                android:layout_column="2"
                android:layout_row="2"
                android:layout_marginBottom="@dimen/grid_margin_bottom"
                android:layout_marginEnd="@dimen/grid_margin_right"
                android:layout_gravity="end"
                android:textColor="@color/primary_text"/>

            <TextView
                android:text="@string/black"
                android:layout_column="0"
                android:layout_row="3"
                android:layout_marginBottom="@dimen/grid_margin_bottom"
                android:layout_marginEnd="@dimen/grid_margin_right"
                android:textColor="@color/primary_text"
                />
            <TextView
                android:id="@+id/black1"
                android:layout_column="1"
                android:layout_row="3"
                android:layout_marginBottom="@dimen/grid_margin_bottom"
                android:layout_marginEnd="@dimen/grid_margin_right"
                android:layout_gravity="end"
                android:textColor="@color/primary_text"/>
            <TextView
                android:id="@+id/black2"
                android:layout_column="2"
                android:layout_row="3"
                android:layout_marginBottom="@dimen/grid_margin_bottom"
                android:layout_marginEnd="@dimen/grid_margin_right"
                android:layout_gravity="end"
                android:textColor="@color/primary_text"/>

            <TextView
                android:text="@string/hispanic"
                android:layout_column="0"
                android:layout_row="4"
                android:layout_marginBottom="@dimen/grid_margin_bottom"
                android:layout_marginEnd="@dimen/grid_margin_right"
                android:textColor="@color/primary_text"
                />
            <TextView
                android:id="@+id/hispanic1"
                android:layout_column="1"
                android:layout_row="4"
                android:layout_marginBottom="@dimen/grid_margin_bottom"
                android:layout_marginEnd="@dimen/grid_margin_right"
                android:layout_gravity="end"
                android:textColor="@color/primary_text"/>
            <TextView
                android:id="@+id/hispanic2"
                android:layout_column="2"
                android:layout_row="4"
                android:layout_marginBottom="@dimen/grid_margin_bottom"
                android:layout_marginEnd="@dimen/grid_margin_right"
                android:layout_gravity="end"
                android:textColor="@color/primary_text"/>

            <TextView
                android:text="@string/pacislander"
                android:layout_column="0"
                android:layout_row="5"
                android:layout_marginBottom="@dimen/grid_margin_bottom"
                android:layout_marginEnd="@dimen/grid_margin_right"
                android:textColor="@color/primary_text"
                />
            <TextView
                android:id="@+id/pacislander1"
                android:layout_column="1"
                android:layout_row="5"
                android:layout_marginBottom="@dimen/grid_margin_bottom"
                android:layout_marginEnd="@dimen/grid_margin_right"
                android:layout_gravity="end"
                android:textColor="@color/primary_text"/>
            <TextView
                android:id="@+id/pacislander2"
                android:layout_column="2"
                android:layout_row="5"
                android:layout_marginBottom="@dimen/grid_margin_bottom"
                android:layout_marginEnd="@dimen/grid_margin_right"
                android:layout_gravity="end"
                android:textColor="@color/primary_text"/>

            <TextView
                android:text="@string/others"
                android:layout_column="0"
                android:layout_row="6"
                android:layout_marginBottom="@dimen/grid_margin_bottom"
                android:layout_marginEnd="@dimen/grid_margin_right"
                android:textColor="@color/primary_text" />
            <TextView
                android:id="@+id/others1"
                android:layout_column="1"
                android:layout_row="6"
                android:layout_marginBottom="@dimen/grid_margin_bottom"
                android:layout_marginEnd="@dimen/grid_margin_right"
                android:layout_gravity="end"
                android:textColor="@color/primary_text"/>
            <TextView
                android:id="@+id/others2"
                android:layout_column="2"
                android:layout_row="6"
                android:layout_marginBottom="@dimen/grid_margin_bottom"
                android:layout_marginEnd="@dimen/grid_margin_right"
                android:layout_gravity="end"
                android:textColor="@color/primary_text"/>

            <TextView
                android:text="@string/white"
                android:layout_column="0"
                android:layout_row="7"
                android:layout_marginBottom="@dimen/grid_margin_bottom"
                android:layout_marginEnd="@dimen/grid_margin_right"
                android:textColor="@color/primary_text"
                />
            <TextView
                android:id="@+id/white1"
                android:layout_column="1"
                android:layout_row="7"
                android:layout_marginBottom="@dimen/grid_margin_bottom"
                android:layout_marginEnd="@dimen/grid_margin_right"
                android:layout_gravity="end"
                android:textColor="@color/primary_text"/>
            <TextView
                android:id="@+id/white2"
                android:layout_column="2"
                android:layout_row="7"
                android:layout_marginBottom="@dimen/grid_margin_bottom"
                android:layout_marginEnd="@dimen/grid_margin_right"
                android:layout_gravity="end"
                android:textColor="@color/primary_text"/>

        </GridLayout>


    </LinearLayout>

</RelativeLayout>